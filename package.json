{
  "name": "ssfe-blog",
  "version": "1.0.0",
  "description": "ssfe博客",
  "main": "index.js",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules --mode production",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules --mode production",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node server.js",
    "dev": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binnull/ssfe-blog.git"
  },
  "keywords": [
    "blog",
    "ssfe"
  ],
  "author": "bianhao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binnull/ssfe-blog/issues"
  },
  "homepage": "https://github.com/binnull/ssfe-blog#readme",
  "dependencies": {
    "express": "^4.16.4",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^2.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lru-cache": "^4.1.3",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.4",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
